{
    "version": 3,
    "cmakeMinimumRequired": {
        "major": 3,
        "minor": 4,
        "patch": 0
    },
    "configurePresets": [
        {
            "name": "conan",
            "displayName": "conan",
            "description": "'conan' configure using 'Visual Studio 17 2022' generator",
            "generator": "Visual Studio 17 2022",
            "cacheVariables": {
                "CMAKE_POLICY_DEFAULT_CMP0091": "NEW"
            },
            "toolchainFile": "./build/conan_toolchain.cmake",
            "binaryDir": "./build"
        }
    ],
    "buildPresets": [
        {
            "name": "debug",
            "configurePreset": "conan",
            "configuration": "Debug"
        },
        {
            "name": "release",
            "configurePreset": "conan",
            "configuration": "Release"
        }
    ]
}