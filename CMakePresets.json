{
    "include": [
        "./build/CMakePresets.json"
    ],
    "version": 6,
    "cmakeMinimumRequired": {
        "major": 3,
        "minor": 4,
        "patch": 0
    },
    "configurePresets": [
        {
            "inherits": "conan-debug",
            "name": "Release",
            "displayName": "Release preset for project.",
            "cacheVariables": {
                "CMAKE_BUILD_TYPE": "Release"
            }
        },
        {
            "inherits": "conan-debug",
            "name": "Debug",
            "displayName": "Debug preset for project.",
            "cacheVariables": {
                "CMAKE_BUILD_TYPE": "Debug"
            }
        },
        {
            "inherits": "conan-debug",
            "name": "RelWithDebInfo",
            "displayName": "RelWithDebInfo preset for project.",
            "cacheVariables": {
                "CMAKE_BUILD_TYPE": "RelWithDebInfo"
            }
        },
        {
            "inherits": "conan-debug",
            "name": "MinSizeRel",
            "displayName": "MinSizeRel preset for project.",
            "cacheVariables": {
                "CMAKE_BUILD_TYPE": "MinSizeRel"
            }
        }
    ],
    "buildPresets": [
        {
            "name": "Build"
        },
        {
            "name": "CleanBuild",
            "cleanFirst": true
        },
        {
            "name": "conan-BuildDefault",
            "inherits": "Build",
            "displayName": "Build",
            "configurePreset": "conan-debug",
            "configuration": "Debug"
        },
        {
            "name": "conan-CleanBuildDefault",
            "inherits": "CleanBuild",
            "displayName": "Clean ReBuild",
            "configurePreset": "conan-debug",
            "configuration": "Debug"
        },
        {
            "name": "BuildRelease",
            "inherits": "Build",
            "displayName": "Build",
            "configurePreset": "Release",
            "configuration": "Release"
        },
        {
            "name": "CleanBuildRelease",
            "inherits": "CleanBuild",
            "displayName": "Clean ReBuild",
            "configurePreset": "Release",
            "configuration": "Release"
        },
        {
            "name": "BuildDebug",
            "inherits": "Build",
            "displayName": "Build",
            "configurePreset": "Debug",
            "configuration": "Debug"
        },
        {
            "name": "CleanBuildDebug",
            "inherits": "CleanBuild",
            "displayName": "Clean ReBuild",
            "configurePreset": "Debug",
            "configuration": "Debug"
        },
        {
            "name": "BuildRelWithDebInfo",
            "inherits": "Build",
            "displayName": "Build",
            "configurePreset": "RelWithDebInfo",
            "configuration": "Debug"
        },
        {
            "name": "CleanBuildRelWithDebInfo",
            "inherits": "CleanBuild",
            "displayName": "Clean ReBuild",
            "configurePreset": "RelWithDebInfo",
            "configuration": "Release"
        },
        {
            "name": "BuildMinSizeRel",
            "inherits": "Build",
            "displayName": "Build",
            "configurePreset": "MinSizeRel",
            "configuration": "Release"
        },
        {
            "name": "CleanBuildMinSizeRel",
            "inherits": "CleanBuild",
            "displayName": "Clean ReBuild",
            "configurePreset": "MinSizeRel",
            "configuration": "Release"
        }
    ]
}